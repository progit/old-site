git fetch gitbook
git rm -r translations
git read-tree --prefix=translations/en -u gitbook/master:en
git read-tree --prefix=translations/de -u gitbook/master:de
git read-tree --prefix=translations/zh -u gitbook/master:zh
git read-tree --prefix=translations/ja -u gitbook/master:ja
git read-tree --prefix=translations/ru -u gitbook/master:ru
